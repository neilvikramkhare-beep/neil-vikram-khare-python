class Order:
    def __init__(self,name,price):
        self.name=name
        self.price=price
        def __gt__(self,odr2):
            return self.price>odr2.price
            odr1 = Order("Vegetables",790)
            odr2 = Order("Friuts",780)
            print(odr1>odr2)
            print(odr2<odr1)
        
