def fibonacci_iterative(n):
    """Generates the Fibonacci sequence up to n terms using a loop."""
    if n <= 0:
        return []
    elif n == 1:
        return [0]
    sequence = [0, 1]
    for i in range(2, n):
        next_num = sequence[i-1] + sequence[i-2]
        sequence.append(next_num)
    return sequence
print(fibonacci_iterative(10)) 
