num1 = 5
num2 = 10
resultint = num1 + num2
print("Integer addition:", {resultint})
print("Type:", {type(resultint)})
str1 = "Hello, "
str2 = "World!"
resultstr = str1 + str2
print("String concatenation:",{resultstr})
print("Type:", {type(resultstr)})
list1 = [1, 2]
list2 = [3, 4]
resultlist = list1 + list2
print("List concatenation:",resultlist) 
print("Type:", {type(resultlist)})
