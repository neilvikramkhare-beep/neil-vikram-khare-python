num1 = 5
num2 = 10
result_int = num1 + num2
print(f"Integer addition: {result_int} (Type: {type(result_int)})")
str1 = "Hello, "
str2 = "World!"
result_str = str1 + str2
print(f"String concatenation: {result_str} (Type: {type(result_str)})")
list1 = [1, 2]
list2 = [3, 4]
result_list = list1 + list2
print(f"List concatenation: {result_list} (Type: {type(result_list)})")
