class Employee:
    def __init__(self, name="Rohan", salary=32000):
        self.name = name
        self.salary = salary
    def displayEmployee(self):
        print(f"My name is: {self.name}")
        print(f"My salary is: {self.salary}")
class Manager:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary
    def displayManager(self):
        print(f"Name of the manager: {self.name}")
        print(f"Salary of the manager: {self.salary}")

e = Employee()
m = Manager("Ramesh", 89000)
if m.salary > e.salary:
    print(f"Manager's salary is greater: {m.salary}")
    m.displayManager()
else:
    print(f"Employee's salary is greater: {e.salary}")
    e.displayEmployee()
