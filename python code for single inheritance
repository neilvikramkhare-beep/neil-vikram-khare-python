class Animal:
    def __init__(self, name):
        self.name = name
    def speak(self):
        return "Makes a sound"
class Dog(Animal):
    def __init__(self, name, breed):
        super().__init__(name)  
        self.breed = breed
    def speak(self):
        return "Barks"
    def fetch(self):
        return f"{self.name} is fetching the ball."
animal_obj = Animal("Generic Animal")
dog_obj = Dog("Buddy", "Golden Retriever")
print(f"{animal_obj.name} says: {animal_obj.speak()}")
print(f"{dog_obj.name} says: {dog_obj.speak()}")
print(dog_obj.fetch())
print(f"{dog_obj.name} is a {dog_obj.breed}.")
