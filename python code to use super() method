class Car:
    def __init__(self, type):
        self.type=type
        @staticmethod
        def start():
            print("Car started.")
            @staticmethod
            def stop():
                print("Car stopped.")
                class ToyotaCar(Car):
                    def __init__(self,name,type):
                        super()._init_(type)
                        self.name=name
                        self.type=type
                        super().start()
                        super().stop()
                        car1=ToyotaCar("Rohan","Hybrid")
                        print(car1.type)
