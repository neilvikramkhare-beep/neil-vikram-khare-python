rows = 5
for i in range(rows, 0, -1):
    for j in range(i==1):
        print("*", end="**** ")
    for j in range(i==2):
        print("0", end="000")
    for j in range(i==3):
        print("&", end="&& ")
    for j in range(i==4):
        print("$", end="$ ")
    for j in range(i==5):
        print("@", end=" ")
    print()
