class Student:
    college_name = "MIT"
    def __init__(self, fullname, marks):
        self.name = fullname
        self.marks = marks
        print("creating new student in the database.")
    def welcome(self):
        print("Welcome student", self.name)
    def get_marks(self):
        return self.marks
s1 = Student("Siraj", 78)
print(s1.name)
print(s1.marks)
s2 = Student("Aryan", 92)
print(s2.name)
print(s2.marks)
print(s2.college_name) 
s1.welcome()
print(s1.get_marks())
