def selectionSort(a):
    n = len(a)
    for i in range(n):
        minindex = i
        for j in range(i + 1, n):
            if a[j] < a[minindex]:
                minindex = j
        a[i], a[minindex] = a[minindex], a[i]
myList = [64, 25, 12, 22, 11]
print("Original list:", myList)
selectionSort(myList)
print("Sorted list:", myList)
